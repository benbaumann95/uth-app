<% if @ticket.persisted? %>
  <%= puts event_path(@event.id) %>
  <%= puts event_url(@event.id, redirect_success: true) %>

  console.log(window.location)


  window.location = "<%= event_url(@event.id)%>";
<% else %>
  document.getElementById("event-modal-<%= @ticket.event.id %>").innerHTML =
    "<%= j render 'add_ticket', event: @ticket.event, ticket: @ticket %>";
<% end %>
