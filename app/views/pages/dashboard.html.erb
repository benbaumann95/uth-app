<%= render 'dashboard_nav' %>

<div class="dashboard-tabs">
  <a class="dashboardtab active" id="listings-tab">
    <div class="dashboard-tab-text">
      <h3><%= @number_listings %></h3>
      <p>Listings</p>
    </div>
  </a>
  <a class="dashboardtab" id="bookings-tab">
    <div class="dashboard-tab-text">
      <h3><%= @number_purchases %></h3>
      <p>Purchased</p>
    </div>
  </a>
  <a class="dashboardtab" id="watching-tab">
    <div class="dashboard-tab-text">
      <h3> <%= @number_watching %></h3>
      <p>Watching</p>
    </div>
  </a>
</div>

<%= render 'dashboard-listings' %>


<%= render 'dashboard-purchases' %>


<%= render 'dashboard-watching', watchlists: @watchlists %>

<%= javascript_pack_tag "dashboard" %>
<% if params[:redirect_success] == "true" %>
  <%= javascript_pack_tag "sweet-alert-buy" %>
<% end %>






