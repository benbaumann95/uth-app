<div class="container">
  <% if current_user[:photo] == nil  %>
    <h1>Please upload a profile photo</h1>
    <% render 'edit_form' %>
  <% else %>
    <div class="container" style=" padding: 10px 0px 10px 0;">
      <%= render 'list_form', event: @event, ticket: @ticket %>
    </div>
  <% end %>
</div>

 <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?libraries=places&key=#{ENV['GOOGLE_API_BROWSER_KEY']}" %>
 <%= javascript_pack_tag "form" %>



